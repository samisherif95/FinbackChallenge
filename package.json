{
  "name": "yellowstone",
  "version": "1.0.0",
  "description": "server side application for bryce",
  "engines": {
    "node": "12.13.1"
  },
  "main": "index.js",
  "repository": "https://github.com/sean-connor/yellowstone",
  "author": "Sean Connor <connorseanp@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "debug": "node --inspect-brk app.js",
