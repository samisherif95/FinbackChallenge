    "lodash": "^4.17.15",
    "mongoose": "^5.9.15",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "postgraphql": "^3.5.6",
    "sequelize": "^4.35.0",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^16.1.0",
    "husky": "^0.14.3",
    "nodemon": "^1.17.1",
